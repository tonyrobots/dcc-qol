{
    "id": "dcc-qol",
    "title": "DCC Quality of Life Improvements",
    "description": "Various automation and quality of life improvements for Dungeon Crawl Classics.",
    "version": "0.1.26",
    "library": "false",
    "manifestPlusVersion": "1.2.0",
    "compatibility": {
        "minimum": 11,
        "verified": 12
    },
    "authors": [
        {
            "name": "tonyrobots",
            "url": "https://github.com/tonyrobots/",
            "discord": "tonyrobots"
        }
    ],
    "relationships": {
        "systems": [
            {
                "id": "dcc",
                "type": "system",
                "compatibility": {
                    "minimum": "0.36.0"
                }
            }
        ],
        "requires": [
            {
                "id": "lib-wrapper",
                "type": "module"
            },
            {
                "id": "socketlib",
                "type": "module"
            }
        ]
    },
    "esmodules": ["scripts/dcc-qol.js"],
    "styles": ["styles/dcc-qol.css"],
    "languages": [
        {
            "lang": "en",
            "name": "English",
            "path": "language/en.json"
        }
    ],
    "url": "#{URL}#",
    "manifest": "#{MANIFEST}#",
    "download": "#{DOWNLOAD}#",
    "license": "LICENSE",
    "readme": "README.md",
    "changelog": "https://github.com/tonyrobots/dcc-qol/blob/main/CHANGELOG.md",
    "socket": true
}
